{
  "title": "Misc",
  "rules": [
    {
      "description": "Shf+F11/F10 to volumn Up/Down",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "f11",
            "modifiers": {
              "mandatory": [
                "shift"
              ]
            }
          },
          "to": [
            {
              "key_code": "volume_increment"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "f10",
            "modifiers": {
              "mandatory": [
                "shift"
              ]
            }
          },
          "to": [
            {
              "key_code": "volume_decrement"
            }
          ]
        }
      ]
    },
    {
      "description": "Shf+F9 to Volume mute",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "f9",
            "modifiers": {
              "mandatory": [
                "shift"
              ]
            }
          },
          "to": [
            {
              "key_code": "mute"
            }
          ]
        }
      ]
    },
    {
      "description": "J+K at the same time to Escape",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "j",
            "modifiers": {
              "mandatory": [
                "k"
              ]
            }
          },
          "to": [
            {
              "key_code": "escape"
            }
          ],
          "to_if_alone": [
            {
              "key_code": "j"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "k",
            "modifiers": {
              "mandatory": [
                "j"
              ]
            }
          },
          "to": [
            {
              "key_code": "escape"
            }
          ],
          "to_if_alone": [
            {
              "key_code": "k"
            }
          ]
        }
      ]
    }
  ]
}
